{
    "name": "FreeRTOS",
    "description": "Real-time operating system for microcontrollers",
    "version": "10.0.0",
    "repository":
    {
        "type": "git",
        "url": "https://github.com/FreeRTOS/FreeRTOS-Kernel.git"
    },
    "license": "MIT",
    "build": {
        "srcDir": ".",
        "includeDir": "include",
        "libArchive": false,
        "flags": [
            "-Iportable/GCC/ARM_CM4F"
        ],
        "srcFilter": [
            "+<*>",
            "-<portable/Common/*>",
            "-<portable/IAR/*>",
            "-<portable/RVDS/*>",
            "-<portable/MemMang/*>",
            "-<watcher.c>"
        ]
    }
}